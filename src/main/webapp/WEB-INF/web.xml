
<web-app>
	<display-name>SpitterProject</display-name>
	<welcome-file-list>
		<welcome-file>home.html</welcome-file>
		<welcome-file>home.htm</welcome-file>
		<welcome-file>home.jsp</welcome-file>
		<welcome-file>default.html</welcome-file>
		<welcome-file>default.htm</welcome-file>
		<welcome-file>default.jsp</welcome-file>
	</welcome-file-list>
  
	<servlet>
		<servlet-name>ViewServlet</servlet-name>
		<servlet-class>spitterpackage.ViewServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ViewAcc</servlet-name>
		<servlet-class>spitterpackage.ViewSpecificServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>ViewServlet</servlet-name>
		<url-pattern>/viewAll</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ViewAcc</servlet-name>
		<url-pattern>/viewSpitter</url-pattern>
	</servlet-mapping>


</web-app>